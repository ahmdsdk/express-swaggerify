{
  "name": "express-swaggerify",
  "version": "1.2.5",
  "description": "Auto-generate OpenAPI 3.0 documentation from Express.js routes with intelligent field detection",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "swaggerify": "dist/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "ts-node src/index.ts",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write src/**/*.ts",
    "prepare": "yarn build",
    "example": "cd examples/basic-express-app && yarn docs:generate"
  },
  "keywords": [
    "swagger",
    "openapi",
    "express",
    "routes",
    "documentation",
    "api",
    "auto-generate",
    "typescript",
    "swagger-ui",
    "api-documentation",
    "express-middleware",
    "nodejs"
  ],
  "author": "Ahmad Sadek",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ahmadsdk/express-swaggerify.git"
  },
  "bugs": {
    "url": "https://github.com/ahmadsdk/express-swaggerify/issues"
  },
  "homepage": "https://github.com/ahmadsdk/express-swaggerify#readme",
  "dependencies": {
    "commander": "^11.1.0",
    "fs-extra": "^11.1.1",
    "glob": "^10.3.10",
    "joi-to-json": "^5.0.0",
    "ts-node": "^10.9.1"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "prettier": "^3.1.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.2"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ]
}
